.selectInput li, .selectInput li.clearfix{
    color: #000;
    font-size: 13px;
    line-height: 35px;
    margin: 0 0 8px 0;
}
.selectInput li ul{
    float: left;
    border: 1px solid #a0a0a0;
    width: 400px;
}
.selectInput li ul li{
    float: left;
    color: #888888;
    padding: 3px 6px;
    margin: 6px;
    line-height: 13px;
    cursor: pointer;
}
.selectInput li ul li.active{
    color: #ffffff;
    background-color: #a0d500;
}
.selectInput div{
    float: left;
    width: 80px;
    text-align: right;
    font-weight: bold;
}
.popupInInput{
    position: absolute;
    top: 30px;
    left: -1px;
    float: left;
    z-index: 100;
    display: none;
}

.generalForm{
    font-size: 13px;
}
.generalForm li{
    position: relative;
    margin-bottom: 16px;
}
.generalForm li label{
    width: 100px;
    margin-left: -100px;
    padding-right: 20px;
    line-height: 30px;
    text-align: right;
    display: inline-block;
    *display: inline;
    zoom: 1;
}
.generalForm li input{
    width: 295px;
    height: 30px;
    color: #888888;
    padding: 5px;
    margin-right: 10px;
    border: 1px solid #aaaaaa;
}
.generalForm li input.hiddenFile{
    position: absolute;
    top: 0;
    left: 24px;
    filter:alpha(opacity:0);
    opacity:0;
}

